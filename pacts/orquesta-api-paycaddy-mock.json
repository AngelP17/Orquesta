{
  "consumer": {
    "name": "orquesta-api"
  },
  "interactions": [
    {
      "description": "a payout creation request",
      "providerStates": [
        {
          "name": "PayCaddy receives payout requests"
        }
      ],
      "request": {
        "body": {
          "amount_cents": "1200000",
          "currency": "PAB",
          "seller_id": "s_001"
        },
        "headers": {
          "Content-Type": "application/json",
          "Idempotency-Key": "test-idempotency-key"
        },
        "matchingRules": {
          "body": {
            "$.amount_cents": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^\\d+$"
                }
              ]
            },
            "$.currency": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^(PAB|USD)$"
                }
              ]
            },
            "$.seller_id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "Idempotency-Key": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^[a-z0-9-]+$"
                }
              ]
            }
          }
        },
        "method": "POST",
        "path": "/payouts"
      },
      "response": {
        "body": {
          "externalId": "pc_123",
          "status": "processing"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.externalId": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^(processing|paid)$"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "14.0.0"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "paycaddy-mock"
  }
}